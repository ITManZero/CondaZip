<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXTextField?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import io.github.palexdev.materialfx.controls.MFXButton?>
<AnchorPane id="root" fx:id="root" onMouseClicked="#click_root" prefHeight="600.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="it.archiver.Controller_Files.Controller_file_chooser_stage">
    <children>
        <TreeView id="tree" fx:id="tree_view" layoutX="21.0" layoutY="80.0" onMouseClicked="#click_tree"
                  prefHeight="466.0" prefWidth="200.0"/>
        <ListView id="list" fx:id="list_view" layoutX="226.0" layoutY="80.0" onMouseClicked="#click_list_view"
                  prefHeight="466.0" prefWidth="359.0"/>
        <MFXButton fx:id="select_button" layoutX="442.0" layoutY="558.0" onMouseClicked="#click_select"
                   prefHeight="25.0" prefWidth="52.0" style="-fx-background-color: #0B6D54;" text="Select"
                   textFill="WHITE"/>
        <MFXButton fx:id="cancel_button" layoutX="515.0" layoutY="558.0" onMouseClicked="#click_cancel"
                   style="-fx-background-color: #2c505e;" text="Cancel" textFill="WHITE"/>
        <!--        "#0f9d58"-->
        <MFXTextField fx:id="currentPath" layoutX="149.0" layoutY="36.0" prefHeight="25.0" prefWidth="384.0"/>
        <MFXButton fx:id="forwardButton" layoutX="71.0" layoutY="36.0" onMouseClicked="#click_forwardButton"
                   prefHeight="25.0" prefWidth="30.0" style="-fx-background-color: #0B566F;" text="-&gt;"/>
        <MFXButton fx:id="backwardButton" layoutX="30.0" layoutY="36.0" onMouseClicked="#click_backwardButton"
                   style="-fx-background-color: #90383A;" text="&lt;-"/>
        <Label fx:id="Title" layoutX="31.0" layoutY="6.0" text="Title" textFill="#0f9d58">
            <font>
                <Font name="Bodoni MT Black" size="18.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
